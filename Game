#include <stdio.h>
int main () {
    double food, water;
    char player[10], fam1[10], fam2[10], fam3[10], fam4[10], members[10];

    printf("Welcome to the Oregon Trail. Please enter your name.\n");
    scanf("%s", player);
    printf("Please enter the names of your other four party members, each seperated by a space.\n");
    scanf("%s %s %s %s", &fam1, &fam2, &fam3, &fam4);
    printf("The names of the people in your party are as follows: %s, %s, %s, %s. Is this correct?(Y/N)\n", fam1, fam2, fam3, fam4);
    scanf("%s", &members);
        if (members == 'n')
            printf("Which party member would you like to change?\n(1)%c\n(2)%c\n(3)%c\n(4)%c\n", fam1, fam2, fam3, fam4);
        else 
        printf("Test %s\n", members);
return 0;
}
